<style>
	@keyframes fly-depart {
		0% {
			transform: translateZ(-1000px);
			opacity: 0;
			filter: blur(5px);
		}
		50% {
			transform: translateZ(0px);
			opacity: 1;
			filter: blur(0px);
		}
		100% {
			transform: translateZ(1000px);
			opacity: 0;
			filter: blur(5px);
		}
	}

	.depart_section {
