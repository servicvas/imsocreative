			}
			&:nth-of-type(25) {
				animation-range: 48% 68%;
			}

			@supports (animation-timeline: scroll()) {
				&:nth-of-type(1) {
					grid-area: 1/1;
				}
				&:nth-of-type(2) {
					grid-area: 1/2;
				}
				&:nth-of-type(3) {
