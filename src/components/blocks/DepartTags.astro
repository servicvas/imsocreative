<style>
	@keyframes fly-depart {
		0% {
			transform: translateZ(-1000px);
			opacity: 0;
			filter: blur(5px);
		}
		50% {
			transform: translateZ(0px);
			opacity: 1;
			filter: blur(0px);
		}
		100% {
			transform: translateZ(1000px);
			opacity: 0;
			filter: blur(5px);
		}
	}

	.depart_section {
		height 60rem;
		perspective: 60rem;
		transform-style: preserve-3d;
		display: grid;
		grid: repeat(4, 25dvh) / repeat(4, 25dvw);
		place-items: center;
		position: absolute;
		top: -6rem;
		overflow: clip;
    pointer-events: none!important;
    @media screen and (width >= 1200px) {
      top: 10rem;
      height: 80rem;
      perspective: 120rem;
    }
		& > .depart_tag {
      font-family: var(--font-family-secondary);
      font-size: var(--font-size-l);
      color: var(--color-brand-s);
			transform-style: preserve-3d;
      opacity: 0;
      transition: opacity 0.3s;
			@supports (animation-timeline: scroll()) and (animation-range: entry-crossing) {
				@media (prefers-reduced-motion: no-preference) {
					animation: fly-depart linear both;
					animation-timeline: scroll(root block);
					will-change: transform, opacity, filter;
				}
			}

			&.special.special {
				grid-row: 2 / span 2;
				grid-column: 2 / span 2;
			}

			&:nth-of-type(1) {
				animation-range: 00% 65%;
			}
			&:nth-of-type(2) {
				animation-range: 10% 94%;
			}
			&:nth-of-type(3) {
				animation-range: 52% 82%;
			}
			&:nth-of-type(4) {
				animation-range: 50% 80%;
			}
			&:nth-of-type(5) {
				animation-range: 45% 75%;
			}
			&:nth-of-type(6) {
				animation-range: -2% 90%;
			}
			&:nth-of-type(7) {
				animation-range: -4% 100%;
			}
			&:nth-of-type(8) {
				animation-range: 0% 60%;
			}
			&:nth-of-type(9) {
				animation-range: -1% 65%;
			}
			&:nth-of-type(10) {
				animation-range: 5% 64%;
			}
			&:nth-of-type(11) {
				animation-range: -10% 55%;
			}
			&:nth-of-type(12) {
				animation-range: 52% 82%;
			}
			&:nth-of-type(13) {
				animation-range: 15% 55%;
			}
			&:nth-of-type(14) {
				animation-range: 7% 57%;
			}
			&:nth-of-type(15) {
				animation-range: 45% 55%;
			}
			&:nth-of-type(16) {
				animation-range: 3% 63%;
			}
			&:nth-of-type(17) {
				animation-range: 7% 97%;
			}
			&:nth-of-type(18) {
				animation-range: 32% 82%;
			}
			&:nth-of-type(19) {
